<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox styleClass="root" alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" stylesheets="@menu.css" fx:controller="io.github.berinamajdancic.controllers.MenuController">
   <children>
      <Label styleClass="game-header" text="SPACE_SHOOTER" />
      <Button styleClass="menu-item" text="RESUME GAME" onAction="#resumeGame" />
        <Button styleClass="menu-item" text="EXIT" onAction="#exitApplication" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
